    <maintainers>
        <maintainer>
            <user>richard</user>
            <name>Richard heyes</name>
            <email>richard@php.net</email>
            <role>lead</role>
        </maintainer>
    </maintainers>
    
    <release>
        <version>2.0.0</version>
        <date>2004-06-26</date>
        <license>BSD</license>
        <state>stable</state>
        <notes>Convert to PHP5 only. PHP4 users should continue with version 1.0.14</notes>
    
        <filelist>
